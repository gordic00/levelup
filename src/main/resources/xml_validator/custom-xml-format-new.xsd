<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:element name="realEstates">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="realEstate" type="realEstate" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
        <xs:unique name="unique_id">
            <xs:selector xpath="realEstate"/>
            <xs:field xpath="id"/>
        </xs:unique>
    </xs:element>

    <xs:complexType name="realEstate">
        <xs:sequence>
            <!--Product id, should be unique for XML and should not be changed on re-import-->
            <xs:element name="id" type="xs:integer"/>

            <!--Product category.-->
            <!--Categories for sale:
                stanovi-prodaja
                kuce-prodaja
                zemljiste-prodaja
                garaze-i-parking-mesta-prodaja
                lokali-prodaja
                poslovni-prostori-prodaja
                poslovne-zgrade-prodaja
                hale-i-magacini-prodaja
                ugostiteljski-objekti-prodaja
                ostalo-prodaja
            -->
            <!--Categories for rent:
                stanovi-iznajmljivanje
                sobe
                kuce-iznajmljivanje
                zemljiste-iznajmljivanje
                garaze-i-parking-mesta-iznajmljivanje
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
                hale-i-magacini-izdavanje
                ugostiteljski-objekti-izdavanje
                ostalo-izdavanje
            -->
            <xs:element name="category">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="stanovi-prodaja"/>
                        <xs:enumeration value="stanovi-iznajmljivanje"/>
                        <xs:enumeration value="kuce-prodaja"/>
                        <xs:enumeration value="kuce-iznajmljivanje"/>
                        <xs:enumeration value="zemljiste-prodaja"/>
                        <xs:enumeration value="zemljiste-iznajmljivanje"/>
                        <xs:enumeration value="garaze-i-parking-mesta-prodaja"/>
                        <xs:enumeration value="garaze-i-parking-mesta-iznajmljivanje"/>
                        <xs:enumeration value="lokali-prodaja"/>
                        <xs:enumeration value="lokali-izdavanje"/>
                        <xs:enumeration value="poslovni-prostori-prodaja"/>
                        <xs:enumeration value="poslovni-prostori-iznajmljivanje"/>
                        <xs:enumeration value="poslovne-zgrade-prodaja"/>
                        <xs:enumeration value="poslovne-zgrade-izdavanje"/>
                        <xs:enumeration value="hale-i-magacini-prodaja"/>
                        <xs:enumeration value="hale-i-magacini-izdavanje"/>
                        <xs:enumeration value="ugostiteljski-objekti-prodaja"/>
                        <xs:enumeration value="ugostiteljski-objekti-izdavanje"/>
                        <xs:enumeration value="ostalo-prodaja"/>
                        <xs:enumeration value="ostalo-izdavanje"/>
                        <xs:enumeration value="sobe"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Product price in EUR currency.-->
            <xs:element name="price">
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:minInclusive value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!-- Location id to be mapped to location code -->
            <xs:element name="location_id" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:minExclusive value="0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!-- Location id to be mapped to location code -->

            <!--Product address street name.-->
            <xs:element name="address" type="xs:string" minOccurs="0"/>
            <!--Product address street number.-->
            <xs:element name="address_no" type="xs:string" minOccurs="0"/>
            <!--Product address apartment.-->
            <xs:element name="flat_no" type="xs:string" minOccurs="0"/>

            <!--Product address longitude.-->
            <xs:element name="longitude" type="xs:decimal" minOccurs="0"/>
            <!--Product address latitude.-->
            <xs:element name="latitude" type="xs:decimal" minOccurs="0"/>

            <!--Product name in rs locale.-->
            <xs:element name="portal_name_rs" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!--Product name in en locale.-->
            <xs:element name="portal_name_en" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!-- Product description in rs locale. If this property is blank, then empty string will be set as description.-->
            <xs:element name="description_rs" type="xs:string" minOccurs="0"/>
            <!-- Product description in en locale. If this property is blank, then empty string will be set as description.-->
            <xs:element name="description_en" type="xs:string" minOccurs="0"/>

            <!--Product phone.-->
            <xs:element name="phone" type="xs:string" minOccurs="0"/>
            <!--Product address email.-->
            <xs:element name="email" type="xs:string" minOccurs="0"/>

            <!--Youtube link.-->
            <xs:element name="youtube_link" type="xs:string" minOccurs="0"/>

            <!--Update product date.-->
            <xs:element name="update" type="xs:dateTime" minOccurs="0"/>

            <!--Product pictures. Allowed multiple photos. The photo with lowest "priority" will be set as main picture on product list page.-->
            <xs:element name="photo" type="photo" minOccurs="0" maxOccurs="unbounded"/>


            <!--Real estate area.-->
            <!--Available units m2, ar. To keep compatibility all areas without units are treated like m2.-->
            <!--Applies to categories
            =================================================
            Categories for sale:
                stanovi-prodaja (m2)
                zemljiste-prodaja (ar)
                garaze-i-parking-mesta-prodaja (m2)
                lokali-prodaja (m2)
                poslovne-zgrade-prodaja (m2)
                hale-i-magacini-prodaja (m2)
                ugostiteljski-objekti-prodaja (m2)
                ostalo-prodaja (m2)
            Categories for rent:
                stanovi-iznajmljivanje (m2)
                zemljiste-iznajmljivanje (ar)
                garaze-i-parking-mesta-prodaja (m2)
                lokali-izdavanje (m2)
                poslovne-zgrade-izdavanje (m2)
                hale-i-magacini-izdavanje (m2)
                ugostiteljski-objekti-izdavanje (m2)
                ostalo-izdavanje (m2)
                sobe (m2)
            -->
            <xs:element name="area" minOccurs="0" type="areaType"/>

            <!--<unit is changed> Real estate object area in ar.-->
            <!--Applies to categories
            =================================================
            Categories for sale:
                kuce-prodaja
                poslovni-prostori-prodaja
            Categories for rent:
                kuce-iznajmljivanje
                poslovni-prostori-iznajmljivanje
            -->
            <xs:element name="object_area" minOccurs="0" type="areaType"/>

            <!--Land area in square meters-->
            <!--Applies to categories
            =================================================
            Categories for sale:
                kuce-prodaja
            Categories for rent:
                kuce-iznajmljivanje
            -->
            <xs:element name="land_area" minOccurs="0" type="areaType"/>

            <!--Real estate type.-->
            <!--Applicability of type by categories
            =================================================
            Categories for sale:
                kuce-prodaja
            Categories for rent:
                kuce-iznajmljivanje
            Types:
                <new> house_type_whole
                <new> house_type_terraced
                <new> house_type_horizontal
                <new> house_type_vertical
            =================================================
            Categories for sale:
                zemljiste-prodaja
            Categories for rent:
                zemljiste-iznajmljivanje
            Types:
                estate_type_building
                estate_type_agricultural
            =================================================
            Categories for sale:
                poslovne-zgrade-prodaja
            Categories for rent:
                poslovne-zgrade-izdavanje
            Types:
                business_buildings_type_whole
                business_buildings_type_partial
            =================================================
            Categories for sale:
                ugostiteljski-objekti-prodaja
            Categories for rent:
                ugostiteljski-objekti-izdavanje
            Types:
                hospitality_type_bar
                hospitality_type_restaurant
                hospitality_type_fast_food
                hospitality_type_playground
                hospitality_type_hotel
                hospitality_type_hostel
            =================================================
            Categories for sale:
                stanovi-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
            Types:
                <removed> estate_type_flat
                <removed> estate_type_flat_in_house
            =================================================
            Categories for rent:
                sobe
            Types:
                estate_type_room_in_flat
                estate_type_room_in_house
            -->
            <xs:element name="type" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="house_type_whole"/>
                        <xs:enumeration value="house_type_terraced"/>
                        <xs:enumeration value="house_type_horizontal"/>
                        <xs:enumeration value="house_type_vertical"/>
                        <xs:enumeration value="estate_type_building"/>
                        <xs:enumeration value="estate_type_agricultural"/>
                        <xs:enumeration value="business_buildings_type_whole"/>
                        <xs:enumeration value="business_buildings_type_partial"/>
                        <xs:enumeration value="estate_type_room_in_flat"/>
                        <xs:enumeration value="estate_type_room_in_house"/>
                        <xs:enumeration value="hospitality_type_bar"/>
                        <xs:enumeration value="hospitality_type_restaurant"/>
                        <xs:enumeration value="hospitality_type_fast_food"/>
                        <xs:enumeration value="hospitality_type_playground"/>
                        <xs:enumeration value="hospitality_type_hotel"/>
                        <xs:enumeration value="hospitality_type_hostel"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Agricultural Land Type.-->
            <!--Applicability of type by categories
            =================================================
            Categories for sale:
                zemljiste-prodaja
            Categories for rent:
                zemljiste-iznajmljivanje
            Types:
                agricultural_land_type_field
                agricultural_land_type_orchard
                agricultural_land_type_vineyard
                agricultural_land_type_forest
            =================================================
            -->
            <xs:element name="agricultural_land_type" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="agricultural_land_type_field"/>
                        <xs:enumeration value="agricultural_land_type_orchard"/>
                        <xs:enumeration value="agricultural_land_type_vineyard"/>
                        <xs:enumeration value="agricultural_land_type_forest"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Building construction type-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                kuce-prodaja
                lokali-prodaja
                poslovni-prostori-prodaja
                hale-i-magacini-prodaja
                ugostiteljski-objekti-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                kuce-iznajmljivanje
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                hale-i-magacini-izdavanje
                ugostiteljski-objekti-izdavanje
                sobe
            Building construction type:
                estate_type_construction_old
                estate_type_construction_new
                <removed> state_type_construction_under
            =================================================
            Categories for sale:
                garaze-i-parking-mesta-prodaja
            Categories for rent:
                garaze-i-parking-mesta-iznajmljivanje
            Construction type:
                construction_type_prefabricated_garage
                construction_type_masonry_garage
                construction_type_parking_place
            -->
            <xs:element name="construction_type" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="estate_type_construction_old"/>
                        <xs:enumeration value="estate_type_construction_new"/>
                        <xs:enumeration value="construction_type_prefabricated_garage"/>
                        <xs:enumeration value="construction_type_masonry_garage"/>
                        <xs:enumeration value="construction_type_parking_place"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Registration.-->
            <!--Available for categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                kuce-prodaja
                garaze-i-parking-mesta-prodaja
                poslovni-prostori-prodaja
                poslovne-zgrade-prodaja
                hale-i-magacini-prodaja
                ugostiteljski-objekti-prodaja
            Filing:
                filing_filed
                filing_unbooked
                <new> filing_in_legalization
            =================================================
            Categories for sale:
                zemljiste-prodaja
            Filing:
                filing_filed
                filing_unbooked
            -->
            <xs:element name="filing" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="filing_filed"/>
                        <xs:enumeration value="filing_unbooked"/>
                        <xs:enumeration value="filing_in_legalization"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Furnished.-->
            <!--Available for categories
            =================================================
            Categories for sale:
               <removed> stanovi-prodaja
               <removed> kuce-prodaja
               <removed> poslovni-prostori-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                kuce-iznajmljivanje
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
                ugostiteljski-objekti-izdavanje
                sobe
            Furnished:
                furnished_full
                furnished_semi
                furnished_empty
            -->
            <xs:element name="furnished" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="furnished_full"/>
                        <xs:enumeration value="furnished_semi"/>
                        <xs:enumeration value="furnished_empty"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Number of bathrooms-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                kuce-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                kuce-iznajmljivanje
            -->
            <xs:element name="bathrooms" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="bathrooms_1"/>
                        <xs:enumeration value="bathrooms_2"/>
                        <xs:enumeration value="bathrooms_3"/>
                        <xs:enumeration value="bathrooms_after_3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Heating-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                kuce-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                kuce-iznajmljievanje
                sobe
             Heating:
                land_heating_central
                <new> land_heating_central_calorimeter
                <removed> land_heating_electricity
                land_heating_floor
                land_heating_floors
                land_heating_ta
                <removed> land_heating_no
                land_heating_gas
                <removed> land_heating_electric_heaters
                land_heating_marble_radiators
                land_heating_norwegian_radiators
                <removed> land_heating_general_boiler_room
                land_heating_solid_fuel_stove
                <removed> land_heating_other
                <new> land_heating_heat_pump
                <new> land_heating_tile_stove
                <new> land_heating_air_condition
            =================================================
            Categories for sale:
                lokali-prodaja
                poslovni-prostori-prodaja
                poslovne-zgrade-prodaja
            Categories for rent:
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
            Heating:
                land_heating_central
                land_heating_central_calorimeter
                land_heating_floor
                land_heating_floors
                land_heating_ta
                land_heating_gas
                land_heating_marble_radiators
                land_heating_norwegian_radiators
                land_heating_heat_pump
                land_heating_air_condition
            =================================================
            Categories for sale:
                ugostiteljski-objekti-prodaja
            Categories for rent:
                ugostiteljski-objekti-izdavanje
            Heating:
                land_heating_central
                land_heating_central_calorimeter
                land_heating_floors
                land_heating_ta
                land_heating_gas
                land_heating_floor
                land_heating_heat_pump
                land_heating_norwegian_radiators
                land_heating_tile_stove
                land_heating_marble_radiators
                land_heating_solid_fuel_stove
            -->
            <xs:element name="heating" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="land_heating_central"/>
                        <xs:enumeration value="land_heating_central_calorimeter"/>
                        <xs:enumeration value="land_heating_floor"/>
                        <xs:enumeration value="land_heating_floors"/>
                        <xs:enumeration value="land_heating_ta"/>
                        <xs:enumeration value="land_heating_gas"/>
                        <xs:enumeration value="land_heating_heat_pump"/>
                        <xs:enumeration value="land_heating_norwegian_radiators"/>
                        <xs:enumeration value="land_heating_tile_stove"/>
                        <xs:enumeration value="land_heating_marble_radiators"/>
                        <xs:enumeration value="land_heating_air_condition"/>
                        <xs:enumeration value="land_heating_solid_fuel_stove"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>


            <!--<new> PDV return-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                stanovi-prodaja
             PDV return:
                yes
                no
            -->
            <xs:element name="pdv_return" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="yes"/>
                        <xs:enumeration value="no"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--<new> Condition -->
            <!--Available for next categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                kuce-prodaja
                lokali-prodaja
                poslovni-prostori-prodaja
                poslovne-zgrade-prodaja
                ugostiteljski-objekti-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                kuce-iznajmljievanje
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
                ugostiteljski-objekti-izdavanje
                sobe
            Condition:
                object_condition_original
                object_condition_renovated
                object_condition_lux
                object_condition_for_renovation
            -->
            <xs:element name="condition" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="object_condition_original"/>
                        <xs:enumeration value="object_condition_renovated"/>
                        <xs:enumeration value="object_condition_lux"/>
                        <xs:enumeration value="object_condition_for_renovation"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--<new> Payment Type -->
            <!--Available for next categories
            =================================================
            Categories for rent:
                stanovi-iznajmljivanje
                kuce-iznajmljievanje
                zemljiste-iznajmljivanje
                garaze-i-parking-mesta-iznajmljivanje
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
                hale-i-magacini-izdavanje
                ugostiteljski-objekti-izdavanje
                ostalo-izdavanje
                sobe
            Payment Type:
                payment_type_monthly
                payment_type_3_months
                payment_type_6_months
                payment_type_12_months
            -->
            <xs:element name="payment_type" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="payment_type_monthly"/>
                        <xs:enumeration value="payment_type_3_months"/>
                        <xs:enumeration value="payment_type_6_months"/>
                        <xs:enumeration value="payment_type_12_months"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--<new> Monthly Utilities -->
            <!--Available for next categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                kuce-prodaja
                lokali-prodaja
                poslovni-prostori-prodaja
                poslovne-zgrade-prodaja
                ugostiteljski-objekti-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                kuce-iznajmljievanje
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
                ugostiteljski-objekti-izdavanje
                sobe
            -->
            <xs:element name="monthly_utilities" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:minExclusive value="0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Advertiser-->
            <!--Available for categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                kuce-prodaja
                garaze-i-parking-mesta-prodaja
                lokali-prodaja
                poslovni-prostori-prodaja
                poslovne-zgrade-prodaja
                ostalo-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                lokali-izdavanje
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
                ostalo-izdavanje
             Advertiser:
                advertiser_agency
                advertiser_owner
                advertiser_investor
             =================================================
             Categories for sale:
                zemljiste-prodaja
                hale-i-magacini-prodaja
                ugostiteljski-objekti-prodaja
             Categories for rent:
                kuce-iznajmljivanje
                zemljiste-iznajmljivanje
                garaze-i-parking-mesta-iznajmljivanje
                hale-i-magacini-izdavanje
                ugostiteljski-objekti-izdavanje
                sobe
             Advertiser:
                advertiser_agency
                advertiser_owner
            -->
            <xs:element name="advertiser" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="advertiser_agency"/>
                        <xs:enumeration value="advertiser_investor"/>
                        <xs:enumeration value="advertiser_owner"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Floor.-->
            <!--Applicability of floor numbers by categories
            =================================================
            Categories for sale:
                stanovi-prodaja
                poslovni-prostori-prodaja
            Categories for rent:
                stanovi-iznajmljivanje
                poslovni-prostori-iznajmljivanje
                sobe
            Floors:
                floor_basement
                <removed> floor_low_ground_floor
                <new> floor_half_basment
                floor_high_floor
                floor_ground_floor
                floor_1
                floor_2
                floor_3
                floor_4
                floor_5
                floor_6
                floor_7
                floor_8
                floor_9
                floor_10
                floor_11
                floor_12
                floor_13
                floor_14
