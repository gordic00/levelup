                four_rooms_estate_structure
                four_and_half_room_estate_structure
                five_rooms_estate_structure
                more_then_five_rooms_estate_structure
            =================================================
            Categories for rent:
                sobe
            Estate structure:
                one_bed_estate_structure
                two_bed_estate_structure
                three_bed_estate_structure
                four_and_more_bed_estate_structure
            -->
            <xs:element name="structure" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="large_studio_estate_structure"/>
                        <xs:enumeration value="one_room_estate_structure"/>
                        <xs:enumeration value="one_and_half_room_estate_structure"/>
                        <xs:enumeration value="two_rooms_estate_structure"/>
                        <xs:enumeration value="two_and_half_room_estate_structure"/>
                        <xs:enumeration value="three_rooms_estate_structure"/>
                        <xs:enumeration value="three_and_half_room_estate_structure"/>
                        <xs:enumeration value="four_rooms_estate_structure"/>
                        <xs:enumeration value="four_and_half_room_estate_structure"/>
                        <xs:enumeration value="five_rooms_estate_structure"/>
                        <xs:enumeration value="more_then_five_rooms_estate_structure"/>
                        <xs:enumeration value="one_bed_estate_structure"/>
                        <xs:enumeration value="two_bed_estate_structure"/>
                        <xs:enumeration value="three_bed_estate_structure"/>
                        <xs:enumeration value="four_and_more_bed_estate_structure"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!-- <new> Number of rooms.-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                kuce-prodaja
            Categories for rent:
                kuce-iznajmljivanje
            Number of rooms:
                number_rooms_one_room
                number_rooms_two_rooms
                number_rooms_three_rooms
                number_rooms_four_rooms
                number_rooms_five_or_more_rooms
            -->
            <xs:element name="number_rooms" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="number_rooms_one_room"/>
                        <xs:enumeration value="number_rooms_two_rooms"/>
                        <xs:enumeration value="number_rooms_three_rooms"/>
                        <xs:enumeration value="number_rooms_four_rooms"/>
                        <xs:enumeration value="number_rooms_five_or_more_rooms"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Facility Purpose-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                kuce-prodaja
            Categories for rent:
                kuce-iznajmljivanje
            -->
            <xs:element name="facility_purpose" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="facility_purpose_cottage"/>
                        <xs:enumeration value="facility_purpose_residential_business_house"/>
                        <xs:enumeration value="facility_purpose_rural_household"/>
                        <xs:enumeration value="facility_purpose_farm"/>
                        <xs:enumeration value="facility_purpose_villa"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Construction Method-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                kuce-prodaja
            -->
            <xs:element name="construction_method" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="construction_method_masonry"/>
                        <xs:enumeration value="construction_method_prefabricated"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Number Phone Lines-->
            <!--Available for next categories
            =================================================
            Categories for rent:
                lokali-izdavanje
            -->
            <xs:element name="number_phone_lines" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:minExclusive value="0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Number Offices-->
            <!--Available for next categories
            =================================================
            Categories for sale:
                poslovni-prostori-prodaja
                poslovne-zgrade-prodaja
                hale-i-magacini-prodaja
            Categories for rent:
                poslovni-prostori-iznajmljivanje
                poslovne-zgrade-izdavanje
                hale-i-magacini-izdavanje
            -->
            <xs:element name="offices" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:minInclusive value="0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <!--Usage-->
            <!--Available for next categories
            =================================================
            Categories for rent:
                sobe
            -->
            <xs:element name="usage" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="sobe_usage_shared_bathroom"/>
                        <xs:enumeration value="sobe_usage_shared_kitchen"/>
                        <xs:enumeration value="sobe_usage_private_bathroom"/>
                        <xs:enumeration value="sobe_usage_private_kitchen"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Product pictures. The photo with lowest "priority" will be set as main picture on product list page.-->
    <xs:complexType name="photo">
        <xs:all>
            <!--Photo url-->
            <xs:element name="url" type="xs:string" minOccurs="0"/>
            <!--Photo priority. The photo with lowest "priority" will be set as main picture on product list page.-->
            <xs:element name="priority" type="xs:integer" minOccurs="0"/>
            <!--Last update photo time. Recommended to use timestamp attribute for improving performance of import.
            Photos that were not updated since last import will not be downloaded.-->
            <xs:element name="timestamp" type="xs:long" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="areaType">
        <xs:simpleContent>
            <xs:extension base="area_unit_values">
                <xs:attribute name="unit" type="area_units"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="area_unit_values">
        <xs:restriction base="xs:decimal">
            <xs:minExclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="area_units">
        <xs:restriction base="xs:string">
            <xs:enumeration value="m2"/>
            <xs:enumeration value="ar"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>
